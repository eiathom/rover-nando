{
  "name": "rover-nando",
  "version": "0.0.1",
  "description": "rover-nando",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "test": "NODE_ENV=test npx jest --verbose --runInBand",
    "build": "NODE_ENV=production rm -rf dist && tsc --sourceMap -p ./",
    "lint": "NODE_ENV=production tslint -c tslint.json $(find ./src -type f -name '*.ts')",
    "main": "NODE_ENV=development `which ts-node` --files ./src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eiathom/rover-nando.git"
  },
  "keywords": [
    "node",
    "typescript"
  ],
  "author": "eiathom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eiathom/rover-nando/issues"
  },
  "homepage": "https://github.com/eiathom/rover-nando#readme",
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-format": "^0.2.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1"
  }
